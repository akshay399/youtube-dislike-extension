{
  "name": "show dislikes",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "declartiveContent", "tabs"],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}
